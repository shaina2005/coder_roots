function Navbar({ isLogin, setIslogin }) {
  const handleLogout = () => {
    setIslogin(false);
  };

  return (
    <nav>
      <a href="/">Home</a>
      {isLogin ? (
        <>
          <a href="/book">Book Room</a>
          <a href="/history">History</a>
          <button onClick={handleLogout}>Logout</button>
        </>
      ) : (
        <>
          <a href="/login">Login</a>
          <a href="/register">Register</a>
        </>
      )}
    </nav>
  );
}
